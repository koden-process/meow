{
  "name": "meow",
  "version": "0.1",
  "description": "Welcome to the world of free sales pipeline management. You can use it setup your sales stages, opportunities and forecast your sales. Your forecast is automatically update whenever you move the deal down the pipeline. Meow software was built to simplify your sales process, giving you a bird's eye view of your entire pipeline.",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && node build/worker.js",
    "build": "rm -rf build && npx tsc --build tsconfig.json",
    "test": "env URL={your-server} npx ava"
  },
  "author": "Matthias Damm",
  "license": "AGPLv3",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "ava": "^5.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.1",
    "luxon": "^3.3.0",
    "mongodb": "^5.4.0",
    "pino": "^8.12.1",
    "tsscmp": "^1.0.6",
    "typeorm": "^0.3.15",
    "typescript": "^4.9.5"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "files": [
      "src/tests/**/*",
      "!src/tests/helpers"
    ]
  }
}
